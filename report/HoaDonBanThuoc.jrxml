	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT HD.MaHD, HD.MaKH, HoTenKhachHang, HD.MaNV, HoTenNhanVien, NgayLapHoaDon, TheBaoHiem, TH.MaThuoc, TenThuoc, LoaiThuoc, DonViThuoc, XuatXu, TenNCC, Soluong, DonGiaBan, SoLuong * DonGiaBan AS ThanhTien, MienGiam
FROM ChiTietHoaDon CT JOIN HoaDon HD ON CT.MaHD = HD.MaHD 
                      JOIN Thuoc TH ON CT.MaThuoc = TH.MaThuoc 
					  JOIN NhaCungCap NCC ON TH.MaNCC = NCC.MaNCC
					  JOIN KhachHang KH ON KH.MaKH = HD.MaKH
					  JOIN NhanVien NV ON NV.MaNV = HD.MaNV
WHERE HD.MaHD = HD.MaHD]]>
	</queryString>
